<% provide(:title, 'All users') %>
<div class="row">
  <div class="center-block">  
    <div class="col-md-8" >
     <h1>All users</h1>

       <%= will_paginate %> 
       <p>user search</p>
       <%= form_tag(users_path, :method => 'get' ) do %>
       <%= text_field_tag :search %>
       <%= submit_tag 'Search', :name => nil %>
       <% end %>
        <ul class="users">
          <% @users.each do |user| %>
          <li>
            <%= link_to user.name, user %>
          </li>
          <% end %>
        </ul>

<%= will_paginate %>
</div> 
</div>